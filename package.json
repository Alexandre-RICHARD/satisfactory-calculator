{
  "type": "module",
  "scripts": {
    "start": "vite --open --mode dev",
    " === Common Test === ": "",
    "global-test": "pnpm check-types && pnpm lint-fix && pnpm unit-test-once",
    "unit-test-watch": "vitest watch --mode dev",
    "unit-test-once": "vitest run  --mode dev",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "check-types": "tsc --noEmit",
    " === Pnpm === ": "",
    "install-dependencies": "pnpm i",
    "update-dependencies": "pnpm update",
    "update-submodule": "git submodule update --init --recursive",
    " === Build === ": "",
    "unit-test-report": "pnpm clean && vitest run --mode dev --reporter=html --coverage.enabled=true && vite preview --outDir report --open",
    "build": "tsc && vite build --mode prod",
    "build-preview": "vite preview --open",
    "clean": "rd /s /q build report || rm -rf build report",
    "clean:all": "rd /s /q build node_modules report || rm -rf build node_modules report"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "vis-data": "^7.1.10",
    "vis-network": "^9.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.13.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.18.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vitest": "0.4.1",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.92.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^6.3.5",
    "vitest": "^2.1.9"
  }
}
